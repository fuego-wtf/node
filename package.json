{
  "name": "@graphyn/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "JavaScript SDK for Graphyn AI",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
